<?xml version="1.0" encoding="UTF-8"?>

<!-- This file specified launch period of apps in TPV windows service -->

<apps>

  <!-- Running in day --> 

   <app name="checkReorder" 	active="0" repeat="0.5" night="0" />
   <app name="processReorder" 	active="0" repeat="1" night="0" />
   <app name="mdiData" 			active="0" repeat="1" night="0" />
   <app name="jetprintData" 	active="0" repeat="60" night="0" />
   <app name="taskOnDemand" 	active="1" repeat="0.5" night="0" />
   
  <!-- Running in night --> 
   
   <app name="archiveJobs" 		active="0" repeat="15" night="1" />
   <app name="cleanJobDb" 		active="0" repeat="120" night="1" />
   
  <!-- Temporary apps --> 
   
   <app name="testService" 		active="0" repeat="1" night="0" />
   
</apps>