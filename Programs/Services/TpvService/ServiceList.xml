<?xml version="1.0" encoding="UTF-8"?>

<!-- This file specified launch period of apps in TPV windows service -->

<apps>
   <app name="checkReorder" 	active="1" repeat="0.5" night="0" />
   <app name="processReorder" 	active="1" repeat="1" night="0" />
   <app name="mdiData" 			active="1" repeat="1" night="0" />
   <app name="jetprintData" 	active="1" repeat="60" night="0" />
   <app name="archiveJobs" 		active="1" repeat="15" night="1" />
   <app name="cleanJobDb" 		active="1" repeat="120" night="1" />

   <app name="testService" 		active="0" repeat="1" night="0" />
</apps>